node {
    stage("Maven Compile"){
	
	    withMaven(maven : 'Maven 3.5.2'){
        sh 'mvn compile' 
        }		 
	}
        
    stage("Maven Test"){
        
        withMaven(maven : 'Maven 3.5.2'){
        sh 'mvn test' 
        }	
    }
      stage("Maven Package"){
        
       withMaven(maven : 'Maven 3.5.2'){
        sh 'mvn package' 
        }	
    }
   echo 'Hello World'
   bat 'echo aditi'
}
